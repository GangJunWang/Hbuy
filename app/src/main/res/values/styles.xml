        <item name="android:background">@drawable/bg_left_angle</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingBottom">@dimen/x10</item>
        <item name="android:paddingTop">@dimen/x10</item>
        <item name="android:paddingRight">@dimen/x20</item>
        <item name="android:paddingLeft">@dimen/x20</item>
        <item name="android:textSize">@dimen/x14</item>
    </style>

    <style name="Security_Center_Page_Layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/color_white</item>
        <item name="android:paddingBottom">@dimen/x10</item>
        <item name="android:paddingTop">@dimen/x10</item>
    </style>

